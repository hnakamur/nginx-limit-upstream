ngx_addon_name=ngx_limit_upstream

if test -n "$ngx_module_link"; then
    ngx_module_name=ngx_http_limit_upstream_module
    ngx_module_deps=
    ngx_module_srcs=$ngx_addon_dir/ngx_http_limit_upstream_module.c
    ngx_module_libs=

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES ngx_http_limit_upstream_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_limit_upstream_module.c"
fi

if [ $STREAM != NO ]; then
    if test -n "$ngx_module_link"; then
        ngx_module_name=ngx_stream_limit_upstream_module
        ngx_module_deps=
        ngx_module_incs=src/stream
        ngx_module_srcs=$ngx_addon_dir/ngx_stream_limit_upstream_module.c
        ngx_module_libs=
        ngx_module_type=STREAM

        . auto/module
    else
        STREAM_MODULES="$STREAM_MODULES ngx_stream_limit_upstream_module"
        NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_stream_limit_upstream_module.c"
    fi
fi
